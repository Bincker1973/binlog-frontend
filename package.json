{
  "name": "binlog-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "NODE_ENV=development && vue-cli-service serve",
    "build": "NODE_ENV=production && vue-cli-service build",
    "server:renderer": "node server-renderer.js",
    "dev:server:renderer": "NODE_ENV=development && node server-renderer.js",
    "build:renderer-client": "NODE_ENV=production && export WEBPACK_TARGET=client && vue-cli-service build",
    "build:renderer-server": "NODE_ENV=production && export WEBPACK_TARGET=server && vue-cli-service build --no-clean",
    "build:renderer": "npm run build:renderer-client && npm run build:renderer-server",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint"
  },
